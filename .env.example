APP_NAME="Socie LALI"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=https://societelali.com

DEBUGBAR_ENABLED=false
TELESCOPE_ENABLED=false

DB_CONNECTION=sqlite
# For sqlite, only DB_DATABASE is used. Others are ignored.
DB_DATABASE=database/database.sqlite
#DB_HOST=127.0.0.1
#DB_PORT=5432
#DB_USERNAME=null
#DB_PASSWORD=null
QUEUE_CONNECTION=sync

# disk
FILESYSTEM_DISK=local
FILAMENT_FILESYSTEM_DISK=public
MEDIA_DISK=public
BACKUP_DISK=local

CACHE_DRIVER=array
SETTINGS_CACHE_ENABLED=false
#MAIL_MAILER=helo

# mailtrap
MAIL_USERNAME=null
MAIL_PASSWORD=null

# gmail email set up
#MAIL_HOST=smtp.googlemail.com
#MAIL_PORT=587
#MAIL_USERNAME=ENTER_YOUR_GMAIL_USERNAME
#MAIL_PASSWORD=ENTER_YOUR_GMAIL_APP_PASSWORD
#MAIL_ENCRYPTION=tls

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false
AWS_URL=null

